[alias]
	aco    = !git add --all && git commit --message
	boom   = !git reset --hard HEAD && git clean -fd
	br     = "!f() { if [ -z ${1+x} ]; then git branch --all; else git branch $@; fi }; f"
	bugfix = checkout bugfix
	ch     = "!f() { if [ -z ${1+x} ]; then git checkout master; else git checkout $@; fi }; f"
	co     = commit
	coa    = commit --amend
	com    = commit --message
	feat   = "!f() { git checkout feature/${1}; }; f"
	friday = !git add --all && git commit --message "It's Friday ðŸŽ‰! https://www.youtube.com/watch?v=kfVsfOSbJY0"
